<Window x:Class="ADONET_hW3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADONET_hW3"
        mc:Ignorable="d"
        Title="Library" Height="450" Width="800">
    <Grid>
        <TabControl Name="Tabcontrol1">
            <TabItem Header="Executor" FontSize="16" Background="Cyan">
                <StackPanel VerticalAlignment="Top">
                    <TextBox Name="txtCommand" Width="500" Height="30" Margin="100"/>
                    <Button  Width="100" Margin="100" Background="Cyan" Foreground="White" Click="btnExecute_Click">Execute</Button>
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
